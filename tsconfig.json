{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ESNext",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "rootDir": ".",
      "baseUrl": ".",
      "paths": {
        "*": ["./*"]
      }
    },
    "include": ["js/**/*", "tina/**/*"],
    "exclude": ["node_modules"]
  }